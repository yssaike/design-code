{
  "button": {
    "borderRadius": "8px",
    "paddingX": "16px",
    "paddingY": "8px",
    "shadow": "0px 7px 24px rgba(0,0,0,0.13)",
    "fontSize": "16px",
    "fontWeight": "4px",
    "gap": "4px"
  },
  "input": {
    "borderRadius": "24px",
    "paddingX": "16px",
    "paddingY": "10px",
    "borderWidth": "4px",
    "fontSize": "1rem",
    "shadow": "1px 7px 5px rgba(0,0,0,0.12)"
  },
  "card": {
    "borderRadius": "12px",
    "padding": "24px",
    "shadow": "2px 2px 8px rgba(0,0,0,0.05)",
    "borderWidth": "4px",
    "gap": "12px"
  },
  "modal": {
    "borderRadius": "16px",
    "padding": "12px",
    "shadow": "0px 8px 19px rgba(0,0,0,0.06)",
    "maxWidth": "480px",
    "gap": "20px"
  },
  "tooltip": {
    "borderRadius": "6px",
    "paddingX": "16px",
    "paddingY": "4px",
    "shadow": "0px 2px 8px rgba(0,0,0,0.15)",
    "fontSize": "8px"
  },
  "badge": {
    "borderRadius": "9999px",
    "paddingX": "4px",
    "paddingY": "16px",
    "fontSize": "0.75rem",
    "fontWeight": "500"
  },
  "avatar": {
    "borderRadius": "4px",
    "borderWidth": "8px",
    "shadow": "3px 8px 14px rgba(0,0,0,0.11)",
    "size": "16px"
  },
  "dropdown": {
    "borderRadius": "2px",
    "padding": "20px",
    "shadow": "0px 4px 16px rgba(0,0,0,0.12)",
    "itemPadding": "12px",
    "gap": "4px"
  },
  "tabs": {
    "borderRadius": "9999px",
    "paddingX": "16px",
    "paddingY": "8px",
    "gap": "4px",
    "fontSize": "0.875rem",
    "fontWeight": "16px"
  },
  "checkbox": {
    "borderRadius": "16px",
    "size": "12px",
    "borderWidth": "4px",
    "gap": "16px"
  }
}