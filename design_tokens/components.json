{
  "button": {
    "borderRadius": "8px",
    "paddingX": "16px",
    "paddingY": "8px",
    "shadow": "0px 1px 3px rgba(0,0,0,0.1)",
    "fontSize": "0.875rem",
    "fontWeight": "500",
    "gap": "8px"
  },
  "input": {
    "borderRadius": "24px",
    "paddingX": "12px",
    "paddingY": "10px",
    "borderWidth": "1px",
    "fontSize": "1rem",
    "shadow": "0px 1px 2px rgba(0,0,0,0.05)"
  },
  "card": {
    "borderRadius": "12px",
    "padding": "24px",
    "shadow": "0px 4px 12px rgba(0,0,0,0.08)",
    "borderWidth": "1px",
    "gap": "16px"
  },
  "modal": {
    "borderRadius": "16px",
    "padding": "24px",
    "shadow": "0px 8px 24px rgba(0,0,0,0.12)",
    "maxWidth": "480px",
    "gap": "20px"
  },
  "tooltip": {
    "borderRadius": "6px",
    "paddingX": "16px",
    "paddingY": "4px",
    "shadow": "0px 2px 8px rgba(0,0,0,0.15)",
    "fontSize": "8px"
  },
  "badge": {
    "borderRadius": "9999px",
    "paddingX": "8px",
    "paddingY": "2px",
    "fontSize": "0.75rem",
    "fontWeight": "500"
  },
  "avatar": {
    "borderRadius": "12px",
    "borderWidth": "1px",
    "shadow": "0px 1px 3px rgba(0,0,0,0.1)",
    "size": "40px"
  },
  "dropdown": {
    "borderRadius": "2px",
    "padding": "4px",
    "shadow": "0px 4px 16px rgba(0,0,0,0.12)",
    "itemPadding": "12px",
    "gap": "4px"
  },
  "tabs": {
    "borderRadius": "2px",
    "paddingX": "16px",
    "paddingY": "8px",
    "gap": "4px",
    "fontSize": "0.875rem",
    "fontWeight": "2px"
  },
  "checkbox": {
    "borderRadius": "4px",
    "size": "4px",
    "borderWidth": "4px",
    "gap": "8px"
  }
}